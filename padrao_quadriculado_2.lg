to linhas_horizontais :lh
	repeat lh [
		fd 50*(12-1)
		bk 50*(12-1)
		pu
		lt 90
		fd 50
		rt 90
		pd
	]
	pu
	lt 90
	bk 50*lh
	pd
end

to linhas_verticais :lh
	repeat 12 [
		fd 50*(lh-1)
		bk 50*(lh-1)
		rt 90
		pu
		fd 50
		pd
		lt 90
	]
	pu
	lt 90
	fd 50*12
	rt 90
	pd
end

to padrao
	rt 90
	linhas_horizontais 5
	linhas_verticais 5
end

padrao