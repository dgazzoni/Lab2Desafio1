to linhas_horizontais
	repeat 4 [
		fd 50*(12-1)
		bk 50*(12-1)
		pu
		lt 90
		fd 50
		rt 90
		pd
	]
	pu
	lt 90
	bk 4*50
	pd
end

to linhas_verticais
	repeat 12 [
		fd 50*(4-1)
		bk 50*(4-1)
		rt 90
		pu
		fd 50
		pd
		lt 90
	]
	pu
	lt 90
	fd 50*12
	rt 90
	pd
end

to padrao
	rt 90
	linhas_horizontais
	linhas_verticais
end

padrao