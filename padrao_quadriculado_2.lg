to linhas_horizontais
	repeat 4 [
		fd 550
		bk 550
		pu
		lt 90
		fd 50
		rt 90
		pd
	]
	pu
	lt 90
	bk 4*50
	pd
end

to linhas_verticais
	repeat 12 [
		fd (4-1)*50
		bk (4-1)*50
		rt 90
		pu
		fd 50
		pd
		lt 90
	]
	pu
	lt 90
	fd 600
	rt 90
	pd
end

to padrao
	rt 90
	linhas_horizontais
	linhas_verticais
end

padrao